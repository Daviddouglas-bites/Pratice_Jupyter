   "source": [
    "lista = [1, 5, 6, 226, 3667, 10, 68, 67889]\n",
    "\n",
    "def bubble_sort(arr):\n",
    "    n = len(arr)  \n",
    "    for i in range(n):\n",
    "        for j in range(0, n - i - 1):\n",
    "            if arr[j] > arr[j + 1]:\n",
    "                arr[j], arr[j + 1] = arr[j + 1], arr[j]\n",
    "    return arr\n",
    "\n",
    "print(bubble_sort(lista))\n"
   ]
  }
